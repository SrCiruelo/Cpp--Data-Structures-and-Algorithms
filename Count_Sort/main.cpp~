void count_sort(int* arr,std::size_t arr_size){
  if(arr_size == 0)return;
  int* end = arr + arr_size;
  //find the largest element
  int smallest = sizeof(int);
  int largest = 0;
  for(int* it{arr};it != end; ++it){
    if((*it)>largest){
      largest = (*it);
    }
    else if((*it)<smallest){
      smallest = (*it);
    }
  }
  std::size_t count_arr_size = largest - smallest;
  if(count_arr_size == 0)return;
  int* count_arr = new int[
  for(int* it{arr};it != end; ++it){
    
  }
}
void print_arr(int* arr,std::size_t arr_size){
  for(int i{0};i<arr_size;++i){
    std::cout<<arr[i]<< "  ";
  }
  std::cout<<std::endl;
}
 
int main(int argv,char** argc) {

  int arr[] = {2,32,1,1,23,32,232};
  print_arr(arr,7);
  insertion_sort(arr,7);
  print_arr(arr,7);
return 0;
}
